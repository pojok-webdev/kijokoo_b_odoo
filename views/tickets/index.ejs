      type:'post',
      data:function(params){
        var query = {
          category_id:$('#smainrootcause').val(),
          search:params.hasOwnProperty('term')?params.term:' ',type:'public'
        }
        console.log('smainroot',query)
        return query
      },
      dataType:'json',
      processResults:function(data){
        console.log('Preso reult',data)
        return data
      }
    }
  })

  }
  $('#smainrootcause').select2({
    ajax:{
      url:'/causecategoryselect2',
